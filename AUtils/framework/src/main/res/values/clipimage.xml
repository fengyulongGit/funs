<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="viewfinder_mask">#B2000000</color>

    <dimen name="padding_common">16dp</dimen>

    <string name="action_clip">裁剪</string>
    <string name="msg_camera_invalid_permission_denied">无法打开相机，可能是权限被拒绝</string>
    <string name="msg_camera_invalid_initial_failed">相机初始化失败，无法打开相机</string>
    <string name="msg_camera_invalid_unknown_error">无法打开相机，未知错误</string>
    <string name="msg_could_not_save_photo">无法保存图片</string>
    <string name="msg_clipping_image">正在裁剪图片</string>

    <!--取景框-->
    <declare-styleable name="ClipImageView">
        <attr name="civHeight" format="integer" />
        <attr name="civWidth" format="integer" />
        <attr name="civTipText" format="string" />
        <attr name="civTipTextSize" format="dimension" />
        <attr name="civMaskColor" format="color" />
        <attr name="civClipPadding" format="dimension" />
        <attr name="civClipRoundCorner" format="dimension|reference" />
        <attr name="civClipCircle" format="boolean" />
    </declare-styleable>
